    {
      "uninstall": [
        {
          "pkgutil": "com.intel.pkg.PowerGadget.*",
          "kext": "com.intel.driver.EnergyDriver"
        }
      ]
    },
    {
      "pkg": [
        "Install Intel Power Gadget.pkg"
      ]
